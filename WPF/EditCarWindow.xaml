<Window x:Class="WPF.EditCarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF"
        mc:Ignorable="d"
        Title="EditCarWindow" Height="414" Width="339">
    <StackPanel Margin="10,10,0,10" HorizontalAlignment="Left" Width="301">
        <Label>Car Name:</Label>
        <TextBox x:Name="NameTextBox" Margin="5"></TextBox>
        <Label>NumberPlate:</Label>
        <TextBox x:Name="NumberplateTextBox" Margin="5"></TextBox>
        <Button Content="Save" Width="113" Margin="10" Click="SaveChanges_Click"/>
    </StackPanel>
</Window>
